<%= stylesheet_link_tag 'new', :media => "all" %>

<div class ="post-container">
  <p class= "title">投稿フォーム</p>
  <%= form_with url: tweets_path do |f| %>  
  <%= f.label :投稿内容 %>
      <br>
      <%= f.text_field :body,size: 140 %>
      <br>
      <div class="field">
          <%= f.label :image %>
          <%= f.file_field :image %>
      </div>
  <%= f.submit "送信" %>
  <br>
  <% end %>
</div>