<h1>Rubytwitter</h1>
<h3>Tweet一覧</h3>
<div class = "tweets-container">
  <% @tweets.each do |t| %> 
    <div class = "tweet">
      <%= t.body %>
      <%= t.created_at %>
      <%= link_to"詳細へ", tweet_path(t.id) %>
      <%= link_to"編集", edit_tweet_path(t.id) %>
      <%= button_to "削除する", tweet_path(t.id), method: :delete %>
    </div>
  <% end %>
<%= link_to "新規投稿ページ",tweets_new_path %>
<%= link_to "Topへ", root_path %>
</div>